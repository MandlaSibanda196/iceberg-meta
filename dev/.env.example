# ── Dev environment variables ──────────────────────────────────────
# Copy to dev/.env (it is gitignored):
#   cp dev/.env.example dev/.env
#
# iceberg-meta loads .env from your working directory automatically.
# Or point to it explicitly: iceberg-meta --env-file dev/.env

# MinIO / S3 credentials
AWS_ACCESS_KEY_ID=admin
AWS_SECRET_ACCESS_KEY=password
AWS_REGION=us-east-1
S3_ENDPOINT=http://localhost:9000

# Iceberg catalog
ICEBERG_CATALOG_URI=sqlite:///catalog/iceberg_catalog.db
ICEBERG_WAREHOUSE=s3://warehouse
